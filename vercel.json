{
  "version": 2,
  "name": "pasal-sathi",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/frontend/icons/$1"
    },
    {
      "src": "/manifest.json",
      "dest": "/frontend/manifest.json"
    },
    {
      "src": "/sw.js",
      "dest": "/frontend/sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "REACT_APP_BACKEND_URL": "@backend_url"
  },
  "build": {
    "env": {
      "REACT_APP_BACKEND_URL": "@backend_url"
    }
  }
}
